<div class="jumbotron fol-style">
  <h2>Search Results</h2>

  <% if @users %>
      <% @users.each do |user| %>
        <div class="follower row">
          <h3><%= link_to user.username, "/users/#{user.id}" %></h3>
        </div>
      <% end -%>
  <% else %>
      <p>No results.</p>
  <% end %>

</div>